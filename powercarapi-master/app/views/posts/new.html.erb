<div>
	<h1>Discuss your idea</h1>
	<%= form_for :post, url: posts_path do |f| %>
		<%= f.label :title %><br>
		<%= f.text_field :title %><br>
		<%= f.label :content %><br>
		<%= f.text_field :content %><br>
		<%= f.label :image %><br>
		<%= f.text_field :image %><br><br>

		<%= f.submit %>
	<% end %>
</div>